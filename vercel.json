{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "turbo build --filter web",
  "cleanUrls": true,
  "framework": "nextjs",
  "functions": {
    "*.ts": {
      "maxDuration": 60
    }
  },
  "installCommand": "pnpm install --frozen-lockfile",
  "trailingSlash": false,
  "name": "neutralpress",
  "outputDirectory": "apps/web/.next"
}
