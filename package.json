{
  "name": "neutral-press",
  "private": true,
  "scripts": {
    "build": "pnpm generate-openapi && pnpm --filter docs generate-api-docs && turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "check-types": "turbo run check-types",
    "generate-openapi": "turbo run generate-openapi"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
